{
  "name": "ostoslista",
  "version": "5.0.0",
  "description": "Shopping list Web application",
  "author": "Rauli Laine <rauli.laine@iki.fi>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RauliL/ostoslista.git"
  },
  "bugs": {
    "url": "https://github.com/RauliL/ostoslista/issues"
  },
  "homepage": "https://github.com/RauliL/ostoslista",
  "engines": {
    "node": "20 || >=22"
  },
  "scripts": {
    "start": "node ./lib/index.js",
    "start:dev": "concurrently 'webpack-cli --watch' 'nodemon --watch ./lib ./lib/index.js'",
    "clean": "rimraf './lib' './public/static'",
    "build": "webpack-cli",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "style:check": "prettier --check './src/**/*.{ts,tsx}'",
    "style:fix": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "vitest",
    "prebuild": "$npm_execpath run clean",
    "prepack": "env NODE_ENV=production $npm_execpath run build"
  },
  "dependencies": {
    "@fvilers/normalize-port": "^1.0.0",
    "@varasto/cache-storage": "^3.0.0",
    "@varasto/express-crud": "^5.2.0",
    "@varasto/fs-storage": "^3.0.0",
    "express": "^5.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@formatjs/ts-transformer": "^3.13.20",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.14.168",
    "@types/morgan": "^1.9.2",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.5.1",
    "axios": "^1.9.0",
    "axios-mock-adapter": "^2.1.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.35.5",
    "happy-dom": "^17.6.3",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.8.4",
    "rimraf": "^6.0.1",
    "swr": "^2.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "vitest": "^3.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
